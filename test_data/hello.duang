// single line comment will be ignored
def hello(){
    print("hello world")
}

/*
* multiple line
* comment
* will be ignored
*/
// support function decl
def foo(){
    // support nested function call 支持省略函数调用末尾的;
    hello();
    print("你好世界")
}

// support function call
foo()