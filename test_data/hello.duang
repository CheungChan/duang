// single line comment will be ignored
fn hello(){
    print("hello world");
    print("终于支持中文喽，你好，世界");
    print("终于支持转义喽，你好，\"世界\"");
    print("试试无参数函数");
    print();
}

/*
* mulitple line
* comment
* will be ignored
*/
// support function decl
fn foo(){
    // support nested function call
    hello();
}

// support function call
foo();